{
    "name": "yacoubalhaidari/organization-chart",
    "description": "A Filament plugin to display organization charts using Highcharts",
    "keywords": [
        "filament",
        "organization-chart",
        "widget",
        "highcharts"
    ],
    "homepage": "https://github.com/yacoubalhaidari/organization-chart",
    "license": "MIT",
    "authors": [
        {
            "name": "Yacoub Al-haidari",
            "email": "yacoub@yacoubalhaidari.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0|^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "YacoubAlhaidari\\OrganizationChart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YacoubAlhaidari\\OrganizationChart\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YacoubAlhaidari\\OrganizationChart\\OrganizationChartServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

